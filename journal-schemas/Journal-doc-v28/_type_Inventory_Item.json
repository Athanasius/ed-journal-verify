{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "_type_Inventory_Item",
    "title": "Frontier Developers Elite Dangerous Journal Inventory Item",
    "description": "An Inventory item.",
    "type": "object",
    "additionalProperties":     false,
    "properties": {
        "Name": {
            "type":             "string",
            "minLength":        1
        },
        "Count": {
            "type":             "integer",
            "minimum":          1
        },
        "Stolen": {
            "type":             "integer",
            "minimum":          0
        }
    },
    "required": [
        "Name",
        "Count"
    ]
}
