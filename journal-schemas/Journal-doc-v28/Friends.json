{
    "type":                     "object",
    "additionalProperties":     false,
    "properties": {
        "timestamp": {
            "type":             "string",
            "format":           "date-time"
        },
        "event": {
            "type":             "string",
            "pattern":          "^Friends$"
        },
        "Status": {
            "type":             "string",
            "enum":             [
                        "Requested",
                        "Declined",
                        "Added",
                        "Lost",
                        "Offline",
                        "Online"
            ]
        },
        "Name": {
            "type":             "string",
            "minLength":        1
        }
    }
}
