{
    "type":                     "object",
    "additionalProperties":     false,
    "properties": {
        "timestamp": {
            "type":             "string",
            "format":           "date-time"
        },
        "event": {
            "type":             "string",
            "const":            "Commander"
        },
        "Commander": {
            "$ref":             "file:journal-schemas/Journal-doc-v28/_type_Commander_Name.json"
        },
        "FID": {
            "$ref":             "file:journal-schemas/Journal-doc-v28/_type_FID.json"
        },
        "Horizons": {
            "type":             "boolean"
        },
        "Ship": {
            "type":             "string",
            "minLength":        1
        },
        "ShipID": {
            "type":             "integer",
            "minimum":          0
        },
        "StartLanded": {
            "type":             "boolean"
        },
        "StartDead": {
            "type":             "boolean"
        },
        "GameMode": {
            "type":             "string",
            "enum": [
                "Open",
                "Solo",
                "Group"
            ]
        },
        "Group": {
            "type":             "string",
            "minLength":        1
        },
        "Credits": {
            "type":             "integer",
            "minimum":          0
        },
        "Loan": {
            "type":             "integer",
            "minimum":          0
        },
        "ShipName": {
            "type":             "string",
            "minLength":        1
        },
        "ShipIdent": {
            "type":             "string",
            "minLength":        1
        },
        "FuelLevel": {
            "type":             "number",
            "minimum":          0.00
        },
        "FuelCapacity": {
            "type":             "number",
            "minimum":          0.00
        }
    },
    "required": [
        "timestamp",
        "event",
        "Commander",
        "FID",
        "Horizons",
        "Ship",
        "ShipID",
        "GameMode",
        "Credits",
        "Loan",
        "ShipName",
        "ShipIdent",
        "FuelLevel",
        "FuelCapacity"
    ]
}
